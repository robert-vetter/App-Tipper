<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tipper.tipperapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Appnr03"
        tools:targetApi="31">
        <activity
            android:name="com.tipper.tipperapp.ResultActivity"
            android:exported="false" />
        <activity
            android:name="com.tipper.tipperapp.Contact"
            android:exported="false" />
        <activity
            android:name="com.tipper.tipperapp.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.tipper.tipperapp.GameDescription"
            android:exported="false" />
        <activity
            android:name="com.tipper.tipperapp.SecondActivity"
            android:exported="false"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name="com.tipper.tipperapp.ActivityStartScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>